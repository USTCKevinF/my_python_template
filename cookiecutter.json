{
  "project_name": "Python Project",
  "repo_name": "{{ cookiecutter.project_name.replace(' ', '-').lower().strip('-') }}",
  "package_name": "{{ cookiecutter.project_name.replace(' ', '_').replace('-', '_').lower() }}",
  "project_description": "Awesome `{{ cookiecutter.repo_name }}` is a Python cli/package created with https://gitlab.com/manoelpqueiroz/galactipy",
  "version": "0.1.0",
  "author": "Manoel Pereira de Queiroz",
  "scm_platform": ["gitlab", "github"],
  "scm_username": "manoelpqueiroz",
  "email": "contact@{{ cookiecutter.scm_username.lower().replace(' ', '-') }}.com",
  "licence": [
    "MIT",
    "BSD-3",
    "GNU GPL v3.0",
    "GNU AGPL v3.0",
    "GNU LGPL v3.0",
    "Mozilla Public License 2.0",
    "Apache Software License 2.0",
    "Not open source"
  ],
  "minimal_python_version": [3.8, 3.9, 3.10, 3.11],
  "use_formatters": true,
  "line_length": 88,
  "use_linters": true,
  "docstring_style": ["numpy", "pep257", "google", "other"],
  "create_example_template": ["cli", "none"],
  "create_docker": true,
  "create_docs": true,

  "_all_python_version": [3.8, 3.9, 3.10, 3.11],
  "__scm_base_url": "https://{{ cookiecutter.scm_platform }}.com/{{ cookiecutter.scm_username }}/{{ cookiecutter.repo_name }}",
  "__scm_link_url": "https://{{ cookiecutter.scm_platform }}.com/{{ cookiecutter.scm_username }}/{{ cookiecutter.repo_name }}{% if cookiecutter.scm_platform == 'gitlab' %}/-{% endif %}",
  "_copy_without_render": [
    "poetry.lock"
  ]
}
